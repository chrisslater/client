{
  "name": "ts-react",
  "version": "1.0.0",
  "description": "React project with Typescript",
  "main": "dist/server/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build:client &&  node dist/server/index.js",
    "dev": "concurrently \"npm run tsc:watch\" \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "npm run dev:server:start",
    "dev:server:start": "nodemon --debug --watch dist",
    "dev:client": "npm run build:client:watch",
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "build:client": "webpack",
    "build:client:watch": "webpack -w"
  },
  "author": "",
  "license": "ISC",
  "metadata": {
    "graphql": {
      "schema": "./data/schema.json"
    }
  },
  "peerDependencies": {
    "apollo-client": "^0.5.0",
    "react": "^15.3.1",
    "redux": "^3.6.0"
  },
  "dependencies": {
    "apollo-client": "^1.9.0-0",
    "graphql-tag": "^0.1.16",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-router": "^7.0.1",
    "koa-static": "^3.0.0",
    "react": "^15.3.1",
    "react-apollo": "^0.5.16",
    "react-dom": "^15.3.1",
    "react-relay": "^0.9.3",
    "source-map-loader": "^0.1.5",
    "ts-loader": "^0.8.2",
    "typescript": "^2.0.2",
    "webpack": "2.1.0-beta.25"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "nodemon": "^1.10.2",
    "tslint": "^3.15.1"
  }
